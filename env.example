# ReportPortal Agent Configuration
# Copy this file to .env and fill in your values

# ReportPortal Host (without https://)
RP_HOST=reportportal.qa.staging.integrator.io

# ReportPortal API Token (get from Profile > API Keys in ReportPortal)
RP_TOKEN=your-api-token-here

# ReportPortal Project Name
RP_PROJECT=staging_suites

# Server Port (optional, default: 8080)
PORT=8080

